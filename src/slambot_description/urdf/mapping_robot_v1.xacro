<link name="middle_left_link"/>

<link name="middle_right_link"/>

<link name="back_right_link_1">
  <inertial>
    <origin xyz="4.163336342344337e-17 -0.010000000000000023 8.673617379884035e-19" rpy="0 0 0"/>
    <mass value="0.5698187556329606"/>
    <inertia ixx="0.000288" iyy="0.000538" izz="0.000288" ixy="0.0" iyz="0.0" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.1 0.084 0.007" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://slambot_description/meshes/back_right_link_1.stl" scale="0.001 0.001 0.001"/>
