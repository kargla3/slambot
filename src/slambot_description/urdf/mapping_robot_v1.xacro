<link name="front_right_link_1">
  <inertial>
    <origin xyz="2.7755575615628914e-17 -0.010000000000000023 9.540979117872439e-18" rpy="0 0 0"/>
    <mass value="0.5698187556329606"/>
    <inertia ixx="0.000288" iyy="0.000538" izz="0.000288" ixy="-0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.1 0.084 0.007" rpy="0 0 0"/>
