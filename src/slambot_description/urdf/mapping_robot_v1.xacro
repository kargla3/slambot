    </geometry>
  </collision>
</link>

<link name="lidar_link"/>

<joint name="Obrot 1" type="continuous">
  <origin xyz="0.1 0.084 -0.007" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="front_left_link_1"/>
  <axis xyz="0.0 -1.0 0.0"/>
</joint>

<joint name="Obrot 2" type="continuous">
  <origin xyz="-0.1 0.084 -0.007" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="back_left_link_1"/>
  <axis xyz="0.0 -1.0 0.0"/>
</joint>

<joint name="Obrot 3" type="continuous">
  <origin xyz="-0.1 -0.084 -0.007" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="back_right_link_1"/>
  <axis xyz="0.0 -1.0 0.0"/>
</joint>

<joint name="Obrot 4" type="continuous">
  <origin xyz="0.1 -0.084 -0.007" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="front_right_link_1"/>
  <axis xyz="0.0 -1.0 0.0"/>
</joint>

<joint name="middle_left_wheel_joint" type="continuous">
  <origin xyz="0.0 0.084 -0.007" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="middle_left_link"/>
  <axis xyz="0 1 0"/>
</joint>

<joint name="middle_right_wheel_joint" type="continuous">
  <origin xyz="0.0 -0.084 -0.007" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="middle_right_link"/>
  <axis xyz="0 1 0"/>
</joint>

<joint name="lidar_joint" type="fixed">
  <origin xyz="0.0 0.0 0.1" rpy="3.14159 0 0"/>
  <parent link="base_link"/>
  <child link="lidar_link"/>
</joint>

<ros2_control name="DiffDriveESPSystem" type="system">
  <hardware>
    <plugin>diffdrive_esp/DiffDriveESPHardware</plugin>
    <param name="device_port">/dev/ttyACM0</param>
    <param name="baud_rate">115200</param>
    <param name="timeout_ms">20</param>
    <param name="wheel_separation">0.336</param>
    <param name="wheel_radius">0.034</param>
  </hardware>

  <joint name="middle_left_wheel_joint">
    <command_interface name="velocity"/>
    <state_interface name="position"/>
    <state_interface name="velocity"/>
  </joint>

  <joint name="middle_right_wheel_joint">
    <command_interface name="velocity"/>
    <state_interface name="position"/>
    <state_interface name="velocity"/>
  </joint>
</ros2_control>

</robot>
