<?xml version="1.0" ?>
<robot name="slambot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find slambot_description)/urdf/materials.xacro" />
<xacro:include filename="$(find slambot_description)/urdf/mapping_robot_v1.trans" />
<link name="base_link">
